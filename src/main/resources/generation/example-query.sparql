PREFIX cims: <http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX cim: <http://iec.ch/TC57/2013/CIM-schema-cim16#>
PREFIX uml: <http://iec.ch/TC57/NonStandard/UML#>
PREFIX md: <http://iec.ch/TC57/61970-552/ModelDescription/1#>

                    SELECT $this ?value WHERE {
                        $this cim:PlanningValue.kind "+RDREQ" ;
                            cim:ActivePowerPlanningValue.value ?toActivate ;
                            cim:PlanningValue.powerSystemResource/^cim:PlanningValue.powerSystemResource ?constraint .
                        ?constraint cim:PlanningValue.kind "+RDV" ;
                                    cim:ActivePowerPlanningValue.value ?value .
                        FILTER (?toActivate > ?value)
                    }